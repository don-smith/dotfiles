#!/bin/zsh

export SSH_AUTH_SOCK=~/.ssh/ssh-agent.$HOSTNAME.sock

gh() {
  eval $(ssh-agent -s -a $SSH_AUTH_SOCK)
  ssh-add ~/.ssh/id_github
}

bb() {
  ssh-add ~/.ssh/id_bitbucket
}

gitlab() {
  ssh-add ~/.ssh/id_gitlab
}

digo() {
  ssh-add ~/.ssh/id_digital_ocean
}

ssbpub() {
  ssh-add ~/.ssh/id_ssb_pub
}

